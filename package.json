{
  "name": "vote-candidate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:local:reset": "dotenv -e .env.local -e .env  -- prisma migrate reset",
    "generate:module": "nest g module",
    "generate:controller": "nest g controller",
    "generate:service": "nest g service",
    "migration": "yarn migration:up && yarn migration:seed && yarn start",
    "migration:seed": "dotenv -- prisma db seed",
    "migration:up": "dotenv -- prisma migrate deploy",
    "migration:local:up": "dotenv -e .env.local -e .env -- prisma migrate dev --preview-feature",
    "migration:local:seed": "dotenv -e .env.local -e .env -- prisma db seed --preview-feature",
    "prisma:studio": "dotenv -e .env.local -e .env -- prisma studio",
    "prisma:generate": "npx prisma generate",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "script:import": "npx ts-node script/import.ts",
    "script:import:local": "dotenv -e .env.local -e .env -- npx ts-node script/import.ts",
    "local:up": "dotenv -e .env.local -e .env -- docker-compose up -d",
    "local:down": "dotenv -e .env.local -e .env -- docker-compose rm -s -f -v",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest --config jest.json --coverage",
    "test:unit:ci": "jest --config jest.json --coverage --maxWorkers=20%",
    "test:watch": "jest --config jest.json --coverage --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=local jest --config ./test/e2e/jest-e2e.json --runInBand --forceExit",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./ormconfig.ts",
    "schema:sync": "npm run build && npm run typeorm -- schema:sync"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*": "pretty-quick --staged"
  },
  "dependencies": {
    "@nestjs/axios": "~2.0.0",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "~9.3.12",
    "@nestjs/config": "~2.3.1",
    "@nestjs/core": "~9.3.12",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "~9.3.12",
    "@nestjs/swagger": "~6.2.1",
    "@nestjs/terminus": "~9.2.1",
    "@node-redis/client": "~1.0.5",
    "@prisma/client": "~4.9.0",
    "@types/humps": "~2.0.2",
    "axios": "~1.3.4",
    "cache-manager": "^5.2.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.0",
    "crypto-ts": "^1.0.2",
    "dayjs": "~1.11.7",
    "dotenv": "~16.0.3",
    "express": "~4.18.2",
    "humps": "~2.0.1",
    "ioredis": "^5.3.2",
    "multer": "^1.4.5-lts.1",
    "pino-http": "~8.3.3",
    "redis": "^4.6.5",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~4.4.1",
    "rxjs": "~7.8.0",
    "typescript": "~4.3.5",
    "uuid": "~9.0.0",
    "xlsx": "~0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "~9.3.0",
    "@nestjs/schematics": "~9.0.4",
    "@nestjs/testing": "~9.3.12",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/express": "~4.17.17",
    "@types/jest": "~29.5.0",
    "@types/node": "~18.15.6",
    "@types/supertest": "~2.0.12",
    "@types/uuid": "~9.0.1",
    "@typescript-eslint/eslint-plugin": "~5.56.0",
    "@typescript-eslint/parser": "~5.56.0",
    "dotenv-cli": "~7.1.0",
    "dotenv-expand": "~10.0.0",
    "eslint": "~8.36.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-config-standard-with-typescript": "~34.0.1",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-n": "~15.6.1",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-sonarjs": "~0.19.0",
    "eslint-plugin-typescript-sort-keys": "~2.3.0",
    "eslint-plugin-unused-imports": "~2.0.0",
    "husky": "~8.0.3",
    "jest": "29.5.0",
    "lint-staged": "~13.2.0",
    "pinst": "~3.0.0",
    "prettier": "~2.8.6",
    "pretty-quick": "~3.1.3",
    "prisma": "~4.9.0",
    "supertest": "~6.3.3",
    "ts-jest": "~29.0.5",
    "ts-node": "~10.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
